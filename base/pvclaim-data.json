{
  "apiVersion": "v1",
  "kind": "PersistentVolumeClaim",
  "metadata": {
    "name": "nfs-claim1-$ORG",
    "labels": {
      "org": "$ORG",
      "billingType":"hourly",
      "region":"us-east",
      "zone":"wdc07"
    }
  },
  "spec": {
    "accessModes": [
      "ReadWriteOnce"
    ],
    "resources": {
      "requests": {
        "storage": "8Gi"
      }
    }
  }
}
